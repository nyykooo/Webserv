server {                   
	listen 127.0.0.1
	# this is going to be ignored
	server_name test
	#error_page 400 pages/4xx/405.html
	error_page 404 pages/4xx/404.html
	#error_page 413 pages/4xx/413.html
	#error_page 405 pages/4xx/405.html
	#error_page 500 /test.html
	client_max_body_size 50000G
	root ./pages
	index index.htm index.htmlx index.html
	#index rickroll-roll.gif
	autoindex off
	cgi_allowed .py /usr/bin/python3
	cgi_allowed .php /usr/bin/php-cgi
	location / {
		#index rfc3875.pdf
		allowed_methods GET
		#autoindex on
		upload_dir ./tester
	}
	location /study {
		root /pages/www
		allowed_methods GET POST DELETE
		autoindex on
	}
	location /post_body {
		client_max_body_size 100
		index youpi.bad_extension
	}
	location /directory/ {
		root ./YoupiBanane
	}
	location = /test.html {
		allowed_methods GET
		return 301 pipipi.html
	}
	location /upload {
		upload_dir ./tester
	}
}
