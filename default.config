

server {                   
	listen 127.0.0.1
	# this is going to be ignored
	server_name test
	error_page 404 400 =503 pages/4xx/405.html
	error_page 413 pages/4xx/413.html
	client_max_body_size 50000G
	#root pages
	index index.htm index.htmlx index.html
	autoindex off
	location / {
		root study
		allowed_methods GET POST DELETE
		autoindex on
	}
	location = test.html {
		allowed_methods GET
	}
}
